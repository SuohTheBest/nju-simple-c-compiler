// sample
int global_var = 10;

// sample
struct Point {
    int x;
    int y;
};

// sample
int print_point(struct Point p);
int sum(int a, int b);
int factorial(int n);

int main() {
    // sample
    int local_var = 5;
    int result;

    // sample
    struct Point p1;
    p1.x = 3;
    p1.y = 4;
    print_point(p1);

    // sample
    result = sum(10, 20);

    // sample
    int num = 5;
    result = factorial(num);


    return 0;
}

// sample
int sum(int a, int b) {
    return a + b;
}

// sample
int factorial(int n) {
    if (n == 0) {
        return 1;
    } else {
        return n * factorial(n - 1);
    }
}
