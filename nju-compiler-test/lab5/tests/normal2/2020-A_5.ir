FUNCTION mod :
PARAM v0
PARAM v1
t0 := v0 / v1
t1 := t0 * v1
t2 := v0 - t1
RETURN t2
FUNCTION main :
READ t3
v2 := t3
ARG #400
ARG v2
t4 := CALL mod
IF t4 == #0 GOTO L0
GOTO L1
LABEL L0 :
v3 := #1
GOTO L2
LABEL L1 :
ARG #4
ARG v2
t5 := CALL mod
IF t5 == #0 GOTO L6
GOTO L4
LABEL L6 :
ARG #100
ARG v2
t6 := CALL mod
IF t6 != #0 GOTO L3
GOTO L4
LABEL L3 :
v3 := #1
GOTO L5
LABEL L4 :
v3 := #0
LABEL L5 :
LABEL L2 :
WRITE v3
RETURN #0
